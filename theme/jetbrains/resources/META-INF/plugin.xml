<idea-plugin>
  <id>oksolar</id>
  <name>OKSolar Theme</name>
  <vendor url="https://github.com/gmifflen/oksolar-jetbrains">gmifflen</vendor>
  <version>1.5</version>

  <depends>com.intellij.modules.lang</depends>
  <idea-version since-build="191.0" />

  <change-notes
    ><![CDATA[ <h3>1.0</h3> <ul> <li>Initial fork from <a
    href="https://github.com/subtheme-dev/solarized">subtheme-dev/solarized</a></li>
    </ul> ]]>
  </change-notes>

  <description
    ><![CDATA[ The OKSolar theme, adapted for JetBrains. Both dark and light themes are
    included. <br/><br/> Contributions are welcome with <a
    href="https://github.com/gmifflen/oksolar-jetbrains">GitHub</a>. ]]>
  </description>

  <extensions defaultExtensionNs="com.intellij">
    <themeProvider id="d9304237-1a55-41ed-beb0-e86bd75439fa" path="/light.theme.json" />
    <themeProvider id="0d30d2d3-c1a4-4620-bab0-3ea13421f0e8" path="/dark.theme.json" />
  </extensions>
</idea-plugin>
